
<!DOCTYPE html>
<!-- {% include "./template/logo_ascii.html.twig" %}-->
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}HUmanBeing{% endblock %}</title>
        {% stylesheets
            'css/framework/bootstrap.min.css'
            'css/framework/jquery-ui.min.css'
            'css/base.css'
            'css/header.css'
            'css/general_menu.css'
        filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% include "./template/header.html.twig" %}
        <div class="container-fluid row">
            <nav class=" col-sm-3">
                {% include "./template/general_menu.html.twig" %}
            </nav>
            <main class="col-sm-9">
                {% block body %}{% endblock %}
            </main>
        </div>
        {% block javascripts %}
            {% javascripts
                'js/framework/jquery-3.2.1.min.js'
                'js/framework/jquery-ui.min.js'
                'js/framework/bootstrap.min.js'
                'js/header.js'
                'js/general_menu.js'
            %}
            <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
</html>
